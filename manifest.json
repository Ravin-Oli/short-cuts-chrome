{
"name": "JavaCuts",
"description": "Chrome Extension For Java Tools",
"version": "2.3.1",
"manifest_version": 3,
"author": "Ravinder Olivier Singh Dadiala", 
"permissions": [
    "activeTab",
    "pageCapture",
    "scripting",
    "system.display",
    "declarativeContent",
    "cookies"
],
"icons": {
    "16": "icons/16px.png",
    "48": "icons/48px.png",
    "128": "icons/128px.png"
},
"action": {
    "default_popup": "/index.html",
    "default_title": "JavaCuts For Chrome",
    "default_icon": {
        "16": "icons/16px.png",
        "48": "icons/48px.png",
        "128": "icons/128px.png"
    }
}
}